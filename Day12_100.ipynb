      }
    },
    {
      "cell_type": "code",
      "source": [
        "def GetSum(n):\n",
        "  sum = 0\n",
        "  for i in str(n):\n",
        "    sum += int(i)\n",
        "  return sum\n",
        "\n",
        "n = int(input(\"enter a number:\"))\n",
        "print(\"the sum of digits of a number is\",GetSum(n))\n",
        "  "
